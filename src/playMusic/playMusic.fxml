<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="50.0" prefWidth="1000.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playMusic.Player">
   <children>
      <ImageView fx:id="last" fitHeight="30.0" fitWidth="30.0" layoutX="30.0" layoutY="10.0" onMouseClicked="#playLastSong" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/lastSong.png" />
         </image>
      </ImageView>
      <ImageView fx:id="control" fitHeight="35.0" fitWidth="35.0" layoutX="85.0" layoutY="7.5" onMouseClicked="#play" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/pause.png" />
         </image>
      </ImageView>
      <ImageView fx:id="next" fitHeight="30.0" fitWidth="30.0" layoutX="140.0" layoutY="10.0" onMouseClicked="#playNextSong" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/nextSong.png" />
         </image>
      </ImageView>
      <Label fx:id="nowTime" layoutX="202.0" layoutY="16.0" text="00:00">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Label fx:id="sumTime" layoutX="643.0" layoutY="16.0" text="00:00">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <ImageView fx:id="volume" fitHeight="12.0" fitWidth="16.0" layoutX="692.0" layoutY="19.0" onMouseClicked="#mute" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/volume.png" />
         </image>
      </ImageView>
      <ImageView fx:id="lyrics" fitHeight="16.0" fitWidth="16.0" layoutX="900.0" layoutY="17.0" onMouseClicked="#showLyrics" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/openLyrics.png" />
         </image>
      </ImageView>
      <ImageView fx:id="love" fitHeight="12.0" fitWidth="22.0" layoutX="868.0" layoutY="19.0" onMouseClicked="#showLove" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/love.png" />
         </image>
      </ImageView>
      <ImageView fx:id="mv" fitHeight="18.7" fitWidth="20.0" layoutX="962.0" layoutY="16.0" onMouseClicked="#playMv" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/bilibili.png" />
         </image>
      </ImageView>
      <ImageView fx:id="playlist" fitHeight="18.0" fitWidth="19.0" layoutX="927.0" layoutY="17.0" onMouseClicked="#showSongList" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/playlist.png" />
         </image>
      </ImageView>
      <MediaView fx:id="mediaview" fitHeight="10.0" fitWidth="10.0" layoutX="967.0" layoutY="22.0" />
   </children>
</AnchorPane>
